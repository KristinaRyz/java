//package qa10_less20_skype;

public class Appl {

	public static void main(String[] args) {
		User u1 = new User("petya", "p1");
		User u2 = new User("petya", "8748065 p1");
		User u3 = new User("vanya", "p2");
		User u4 = new User("sveta", "p1");
		
		Auth users = new Auth();
		System.out.println(users.addUser(u1));
		System.out.println(users.addUser(u2));
		System.out.println(users.addUser(u3));
		System.out.println(users.addUser(u4));
		
	 
		users.display();
		
		System.out.println(users.deleteUser(u2));
		users.display();
		System.out.println(users.deleteUser(new User("anya", "superpass")));
		users.display();
		System.out.println(users.replacePassword(new User("vanya","newPass")));
		users.display();
		System.out.println(users.replacePassword(new User("vanya","newPass")));

	}

}
