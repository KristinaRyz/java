
public class RefTypes {

	public static void main(String[] args) {
		int[] ar1 = {7,4,2,1,8};
		int b = ar1[1];//[][][][4] perekopirovali znachenie 
		ar1=null;
		System.out.println(b);
		
		
		String[] ar2 = {"Hello", "Bye", "Money"};
		String b1 = ar2[1];
		ar2 = null;
		System.out.println(b1);
	}

}
