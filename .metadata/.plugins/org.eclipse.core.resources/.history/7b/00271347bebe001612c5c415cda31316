
public class Tests {

	public static int sumEvensArray(int ar[]){
		int sum = 0;
		for(int i = 0; i<ar.length; i++){
			if(ar[i]%2==0){
				sum += ar[i];
			}
		}
		return sum;
	}

	
	
	public static int maxToEnd(int ar[]){
		int max = ar[0]; maxInd = 0;
		for(int i = 0; i<ar.length; i++){
			if(ar[i]>ar[i+1])
				ar[i]=max;
			maxInd = i;
		}
		return maxInd;
	}
}
