
public class Node {//eto yacheika linked lista
private int value;
private Node next;// next - ssilka tipa node


public int getValue() {
	return value;
}
public Node(int value) {
	super();
	this.value = value;
}
public void setValue(int value) {
	this.value = value;
}
public Node getNext() {
	return next;
}
public void setNext(Node next) {
	this.next = next;
}



}
